% Bibliography Template for Working Papers
%
% For arXiv preprints, use @misc with eprint and archivePrefix fields
% This ensures proper formatting and makes it easy to update when papers are published
%
% Names are written in natural order (First Last)

% Example arXiv citation
@misc{author2024example,
    author = {First Author and Second Coauthor},
    title = {An Example Paper Title},
    year = {2024},
    eprint = {2401.12345},
    archivePrefix = {arXiv},
    primaryClass = {math.PR}
}

% Example published article
@article{author2023published,
    author = {First Author},
    title = {A Published Result},
    journal = {Journal Name},
    volume = {42},
    number = {3},
    pages = {123--145},
    year = {2023},
    doi = {10.1234/example.doi}
}

% Example book
@book{author2020book,
    author = {First Author},
    title = {Book Title},
    publisher = {Publisher Name},
    year = {2020},
    edition = {2nd}
}
